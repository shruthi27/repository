import java.util.Scanner;

public class addition 
{ 
	public static void main(String s[])
	
	{
		int a,b,sum;
		System.out.println("enter two no's");
		Scanner sc=new Scanner(System.in);
		a=sc.nextInt();
		b=sc.nextInt();
		sum=a-~b-1;
		System.out.println("addition of"+a+"and"+b+" is: "+sum);
		
		
		
	}

}
