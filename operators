public class operator 
{
	public static void main(String s[])
	{
		int a=20,b;
		b= a-- - --a;
		System.out.println("b value is"+b);
	
		int c;
	    c= a--;
		System.out.println("c value is"+c);
		
		int d;
		d= a>>2;
		System.out.println("d value is"+d);
		
		int e;
		e= a&b;
		System.out.println("e value is"+e);
	}

	
